<h1>Showing the list</h1>

<table>
  <tr>
  <th>title</th>
  <th>description</th>
  <th>student_id</th>
  <th colspan="3">action</th>
  </tr>
  <% @list.each do |f| %>
  <tr>
    <td><%= f.title %></td>
    <td><%= f.description %></td>
    <td><%= f.student_id %></td>
    <td><%= link_to 'Edit', edit_list_path(f) %></td>
    <td><%= link_to 'Show', list_path(f) %></td>
    <td><%= link_to 'Delete', list_path(f), method: :delete %></td>
    </tr>
    <% end %>

</table>
<%= link_to 'New', new_list_path(@list) %>
